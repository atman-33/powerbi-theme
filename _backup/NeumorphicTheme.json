{
    "name": "Neumorphic Theme",
    "visualStyles": {
        "*": {
            "*": {
                "title": [
                    {
                        "bold": true
                    }
                ],
                "background": [
                    {
                        "comment": "各ビジュアルの背景",
                        "color": {
                            "solid": {
                                "color": "#E5E5E5"
                            }
                        }
                    }
                ],
                "border": [
                    {
                        "comment": "全般 > 効果 > 背景",
                        "show": true,
                        "color": {
                            "solid": {
                                "color": "#E5E5E5"
                            }
                        },
                        "radius": 20
                    }
                ],
                "dropShadow": [
                    {
                        "comment": "全般 > 効果 > 影",
                        "show": true,
                        "position": "Outer",
                        "preset": "Custom",
                        "shadowSpread": 5,
                        "shadowBlur": 50,
                        "angle": 45,
                        "shadowDistance": 28,
                        "transparency": 84
                    }
                ]
            }
        },
        "page": {
            "*": {
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#E5E5E5"
                            }
                        },
                        "transparency": 100
                    }
                ],
                "outspace": [
                    {
                        "color": {
                            "solid": {
                                "color": "#E5E5E5"
                            }
                        }
                    }
                ]
            }
        },
        "slicer": {
            "*": {
                "header": [
                    {
                        "bold": true
                    }
                ],
                "items": [
                    {
                        "comment": [
                            "outline: 全般 > 値 > 罫線 > ex. 0:無 1:上 2:右 4:下 8:左",
                            "色 #00000000 は透明を意味する"
                        ],
                        "outlineStyle": 0,
                        "background": {
                            "solid": {
                                "color": "#00000000"
                            }
                        }
                    }
                ],
                "border": [
                    {
                        "comment": "全般 > 効果 > 背景",
                        "show": true,
                        "color": {
                            "solid": {
                                "color": "#E5E5E5"
                            }
                        },
                        "radius": 20
                    }
                ],
                "dropShadow": [
                    {
                        "comment": "全般 > 効果 > 影",
                        "show": true,
                        "position": "Inner",
                        "preset": "Custom",
                        "shadowSpread": 5,
                        "shadowBlur": 50,
                        "angle": 45,
                        "shadowDistance": 28,
                        "transparency": 84
                    }
                ]
            }
        },
        "gauge": {
            "*": {
                "target": [
                    {
                        "fontSize": 12,
                        "bold": true,
                        "color": {
                            "solid": {
                                "color": "#FF0000"
                            }
                        }
                    }                    
                ],
                "calloutValue": [
                    {
                        "fontSize": 20,
                        "bold": true,
                        "color": {
                            "solid": {
                                "color": "#0400FF"
                            }
                        }
                    }
                ]
            }
            
        },
        "card": {
            "*": {
                "labels": [
                    {
                        "fontSize": 24,
                        "color": {
                            "solid": {
                                "color": "#0400FF"
                            }
                        }
                    }
                ],
                "categoryLabels": [
                    {
                        "fontSize": 20
                    }
                ]
            }
        },
        "tableEx": {
            "*": {
                "grid": [
                    {
                        "gridVertical": true,
                        "rowPadding": 3,
                        "outlineColor": {
                            "solid": {
                                "color": "#15CAB6"
                            }
                        }
                    }
                ],
                "columnHeaders": [
                    {
                        "backColor": {
                            "solid": {
                                "color": "#E5E5E5"
                            }
                        }
                    }

                ],
                "values": [
                    {
                        "backColor": {
                            "solid": {
                                "color": "#E5E5E5"
                            }

                        },
                        "backColorPrimary": {
                            "solid": {
                                "color": "#E5E5E5"
                            }

                        },
                        "backColorSecondary": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        }
                    }
                ]
            }
        },
        "textbox": {
            "*": {
                "title": [
                    {
                        "fontSize": 20,
                        "bold": true
                    }
                ],
                "padding": [
                    {
                        "top": 10,
                        "left": 10
                    }
                ],
                "background": [
                    {
                        "comment": "テキストボックスの背景",
                        "color": {
                            "solid": {
                                "color": "#E5E5E5"
                            }
                        }
                    }
                ],
                "border": [
                    {
                        "comment": "全般 > 効果 > 背景",
                        "show": false,
                        "color": {
                            "solid": {
                                "color": "#E5E5E5"
                            }
                        },
                        "radius": 30
                    }
                ],
                "dropShadow": [
                    {
                        "comment": "全般 > 効果 > 影",
                        "show": false,
                        "position": "Inner",
                        "preset": "Custom",
                        "shadowSpread": 5,
                        "shadowBlur": 50,
                        "angle": 45,
                        "shadowDistance": 28,
                        "transparency": 84
                    }
                ]
            }
        }
    },
    "dataColors": [
        "#15CAB6",
        "#007FFF",
        "#F6B53D",
        "#EF8A5A",
        "#E85E76",
        "#696CB5",
        "#0F488C",
        "#323447",
        "#3599B8",
        "#DFBFBF",
        "#4AC5BB",
        "#5F6B6D",
        "#FB8281",
        "#F4D25A",
        "#7F898A",
        "#A4DDEE",
        "#FDAB89",
        "#B687AC",
        "#28738A",
        "#A78F8F",
        "#168980",
        "#293537",
        "#BB4A4A",
        "#B59525",
        "#475052",
        "#6A9FB0",
        "#BD7150",
        "#7B4F71",
        "#1B4D5C",
        "#706060",
        "#0F5C55",
        "#1C2325"
    ],
    "textClasses": {
        "label": {
            "fontFace": "wf_standard-font, helvetica, arial, sans-serif",
            "color": "#323447",
            "fontSize": 12
        },
        "callout": {
            "color": "#323447",
            "fontSize": 9,
            "fontFace": "'Segoe UI Bold', wf_segoe-ui_bold, helvetica, arial, sans-serif"
        },
        "title": {
            "color": "#666666",
            "fontFace": "'Segoe UI Bold', wf_segoe-ui_bold, helvetica, arial, sans-serif",
            "fontSize": 12
        },
        "header": {
            "color": "#323447",
            "fontSize": 12,
            "fontFace": "'Segoe UI Semibold', wf_segoe-ui_semibold, helvetica, arial, sans-serif"
        }
    },
    "bad": "#E85E76",
    "neutral": "#CCCCCC",
    "good": "#15CAB6",
    "minimum": "#E85E76",
    "center": "#CCCCCC",
    "maximum": "#15CAB6",
    "foreground": "#323447",
    "background": "#F8FAFC",
    "foregroundNeutralSecondary": "#808080"
}